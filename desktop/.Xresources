Xcursor.theme: Human
Xcursor.size:  24
Xft.antialias: 1
Xft.autohint:  0
Xft.dpi:       120
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba:      rgb

xterm*faceName:   xft:ubuntu mono-13
xterm*background: black
xterm*foreground: white

urxvt.font: \
        xft:DeJaVu Sans Mono-11,\
        xft:Bitstream Vera Sans Mono-11,\
        xft:Liberation Mono-11,\
        xft:Code2000-11
urxvt.letterSpace:                 -2
urxvt.termName:                    rxvt-unicode-256color
urxvt.depth:                       32
urxvt.background:                  [100]#000000
urxvt.foreground:                  [100]#FFFFFF
urxvt.intensitystyles:             false
urxvt.scrollstyle:                 plain
urxvt.scrollBar:                   true
urxvt.secondaryScreen:             true
urxvt.secondaryScroll:             true
urxvt.secondaryWheel:              true
urxvt.scrollTtyOutput:             false
urxvt.scrollWithBuffer:            true
urxvt.scrollTtyKeypress:           true
urxvt.urgentOnBell:                true
urxvt.saveLines:                   10000
urxvt.iso14755:                    false
! C-Tab and C-S-Tab to F19, F20
urxvt.keysym.Control-Tab:          \033[33~
urxvt.keysym.Control-ISO_Left_Tab: \033[34~
! tmux extension: http://anti.teamidiot.de/static/nei/*/Code/urxvt/osc-xterm-clipboard
urxvt.perl-ext-common:             default,font-size,osc-xterm-clipboard,url-select,keyboard-select
urxvt.url-select.launcher:         firefox
urxvt.url-select.underline:        true
urxvt.keysym.M-s:                  perl:keyboard-select:search
urxvt.keysym.M-v:                  perl:keyboard-select:activate
urxvt.keysym.M-u:                  perl:url-select:select_next
urxvt.keysym.M-plus:               perl:font-size:increase
urxvt.keysym.M-minus:              perl:font-size:decrease

rofi.opacity:                        100
rofi.width:                          50
rofi.lines:                          15
rofi.columns:                        2
rofi.font:                           Aller 11
rofi.color-enabled: true
rofi.color-window: #454545, #262626, #262626
rofi.color-normal: #454545, #dfdfdf, #505050, #666666, #f2f2f2
rofi.color-active: #454545, #f07746, #505050, #666666, #f07746
rofi.color-urgent: #546acf, #f2f2f2, #546acf, #8092e7, #f2f2f2
rofi.fuzzy:                          true

