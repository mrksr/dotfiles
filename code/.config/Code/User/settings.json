{
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Base16 Dark Railscasts",
    "workbench.iconTheme": "material-icon-theme",
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "keyboard.dispatch": "keyCode",
    "python.formatting.provider": "black",
    "editor.fontFamily": "'FuraCode Nerd Font', 'Fira Code', 'FiraCode NF', 'monospace', monospace, Consolas, 'Droid Sans Mono', 'Droid Sans Fallback'",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": true,
    "latex-workshop.intellisense.citation.label": "title",
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (latexmkrc)",
            "tools": [
                "latexmk_rconly"
            ]
        },
        {
            "name": "Make",
            "tools": [
                "make"
            ],
            "args": [
                "-j"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "make",
            "command": "make",
            "args": [
                "%DOC%.pdf"
            ],
            "env": {}
        }
    ],
    "diffEditor.renderSideBySide": true,
    "better-comments.highlightPlainText": true,
    "editor.rulers": [
        80,
    ],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.detectIndentation": false,
    "python.dataScience.askForKernelRestart": false,
    "editor.wordWrap": "wordWrapColumn",
    "latex-workshop.view.pdf.viewer": "tab",

}
